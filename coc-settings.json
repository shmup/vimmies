{
  "suggest.autoTrigger": "none",
  "suggest.noselect": true,
  "html.autoClosingTags": false,
  "html.autoCreateQuotes": false,
  "diagnostic.enable": true,
  "python.pythonPath": "/usr/bin/python",
  "highlight.disableLanguages": ["text"],
  "list.height": 100,
  "pyright.inlayHints.variableTypes": false,
  "pyright.inlayHints.parameterTypes": false,
  "pyright.inlayHints.functionReturnTypes": true,
  "zig.enabled": true,
  "zig.startUpMessage": true,
  "zig.path": "~/.zls/zls",
  "colors.enable": true,
  "highlight.trace": "verbose",
  "zig.debugLog": false,
  "codeLens.enable": true,
  "elixir.pathToElixirLS": "~/.elixir-ls/release/language_server.sh",
  "languageserver": {
    "commonlisp": {
      "command": "cl-lsp",
      "filetypes": ["lisp"]
    },
    "vala": {
      "command": "vala-language-server",
      "filetypes": ["vala", "genie"]
    },
    "cs": {
      "command": "csharp-ls",
      "filetypes": ["cs"]
    },
    "bash": {
      "command": "bash-language-server",
      "args": ["start"],
      "filetypes": ["sh"],
      "ignoredRootPaths": ["~"]
    },
    "racket": {
      "command": "racket",
      "args": ["--lib", "racket-langserver"],
      "filetypes": ["racket"],
      "ignoredRootPaths": ["~"]
    },
    "raku": {
      "command": "node",
      "args": ["/home/jtm/src/RakuNavigator/server/out/server.js --stdio"],
      "filetypes": ["raku", "rakumod", "rakudoc", "rakutest", "t", "pl6", "pm6", "p6", "t6"]
    }
  },
  "Lua.diagnostics.globals": [
    "ProgramPort",
    "DecodeBase64",
    "EncodeBase64",
    "EncodeJson",
    "EncodeLua",
    "EncodeUrl",
    "EscapePath",
    "FormatIp",
    "GetCryptoHash",
    "GetLastModifiedTime",
    "GetLogLevel",
    "GetPass",
    "GetRandomBytes",
    "GetTime",
    "GetUser",
    "GetZipPaths",
    "LaunchBrowser",
    "LoadAsset",
    "ParseHttpDateTime",
    "ParseUrl",
    "ProgramHeartbeatInterval",
    "ProgramLogPath",
    "ServeAsset",
    "ServeIndex",
    "argon2",
    "kLogDebug",
    "kLogError",
    "kLogInfo",
    "kLogVerbose",
    "kLogWarn",
    "runSchedule",
    "unix",
    "lsqlite3"
  ],
  "rust-analyzer.workspace.ignoredFolders": [
    "$HOME",
    "$HOME/.cargo/**",
    "$HOME/.rustup/**"
  ],
  "tsserver.enable": true,
  "typescript.implementationsCodeLens.enabled": true,
  "typescript.referencesCodeLens.showOnAllFunctions": false,
  "diagnostic.checkCurrentLine": true,
  "javascript.format.enable": true,
  "javascript.preferences.importModuleSpecifier": "relative",
  "javascript.showUnused": true,
  "javascript.suggest.enabled": true,
  "javascript.suggestionActions.enabled": true,
  "javascript.validate.enable": true,
  "svelte.enable-ts-plugin": false,
  "tsserver.disableAutomaticTypeAcquisition": false,
  "tsserver.enableJavascript": true,
  "tsserver.implicitProjectConfig.checkJs": true,
  "tsserver.implicitProjectConfig.experimentalDecorators": true,
  "tsserver.implicitProjectConfig.module": "commonjs",
  "tsserver.implicitProjectConfig.target": "ES2020",
  "tsserver.log": "verbose",
  "tsserver.maxTsServerMemory": 4096,
  "tsserver.tsdk": null,
  "tsserver.useLocalTsdk": false,
  "typescript.preferences.importModuleSpecifier": "relative",
  "typescript.referencesCodeLens.enabled": true,
  "typescript.suggest.enabled": true,
  "typescript.suggestionActions.enabled": true,
  "typescript.validate.enable": true,
}
